package googlecodejam;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;
import java.util.stream.Stream;

public class PrimeiroDesafio {

	public static void main(String[] args) throws Exception {
		final FileReader reader = new FileReader(new File("src/main/resources/A-small-practice.in"));
		final BufferedReader br = new BufferedReader(reader);
		
		final Stream<String> lines = br.lines();
		
		String linha = br.readLine();

		int contador = 0;
		int casosDeTeste = linha != null && linha != "" ? Integer.parseInt(linha) : 0;

		while(contador != 0 && contador <= casosDeTeste) {
			System.out.printf("%s\n", linha);
			linha = br.readLine();
			casosDeTeste++;
		}
		
		br.close();
	}

}
