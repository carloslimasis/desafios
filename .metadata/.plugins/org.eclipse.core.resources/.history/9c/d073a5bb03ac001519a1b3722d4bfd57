package googlecodejam;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;

public class PrimeiroDesafio {

	public static void main(String[] args) throws Exception {
		final FileReader reader = new FileReader(new File("src/main/resources/example.in"));
		final BufferedReader br = new BufferedReader(reader);
		
		int number = 0;
		int contador = 0;
		
		String[] prices = null;
		
		String line = br.readLine();
		
		int caso = 1;
		
		while(line != null){
			if(number == 0 && contador != 0){
				number = Integer.parseInt(line);
			}else if(number == 0 && contador != 0){
				number = Integer.parseInt(line);
			}else if(prices == null && contador != 0){
				prices = line.split(" ");
				boolean casoResolvido = false;
				Integer[] precosToInt = new Integer[prices.length];
				for (int i = 0; i < prices.length; i++) {
					precosToInt[i] = Integer.parseInt(prices[i]);
				}
				
				for (int i = 0; i < precosToInt.length; i++) {
					if(casoResolvido){
						break;
					}
					Integer precoI = precosToInt[i];
					for (int j = 0; j < precosToInt.length; j++) {
						Integer precoJ = precosToInt[j];
						
						if((i != j) && (precoI + precoJ == number)){
							System.out.println("Case #" + caso + ": " + (i+1) + " " + (j+1));
							caso++;
							casoResolvido = true;
							number = 0;
							prices = null;
							break;
						}
					}
				}
			}
			
			line = br.readLine();
			contador++;
		}
		
		br.close();
	}

}
