package googlecodejam;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;

public class PrimeiroDesafio {

	public static void main(String[] args) throws Exception {
		final FileReader reader = new FileReader(new File("src/main/resources/A-small-practice.in"));
		final BufferedReader br = new BufferedReader(reader);
		
		int credito = 0;
		int contador = 0;
		int produtos = 0;
		
		String[] precos = null;
		
		String linha = br.readLine();
		
		int caso = 1;
		
		while(linha != null){
			
			boolean jaFoiCredito = false;
			boolean jaFoiProduto = false;

			System.out.printf("%s\n", linha);
			if(contador != 0 && contador % 1 == 0 && contador % 2 != 0){
				credito = Integer.parseInt(linha);
				jaFoiCredito = true;
			}else if(jaFoiCredito && produtos == 0 && contador % 2 == 0){
				produtos = Integer.parseInt(linha);
				jaFoiProduto = true;
			}else if(jaFoiProduto && precos == null && contador % 2 != 0){
				boolean casoResolvido = false;
 				precos = linha.split(" ");
				Integer[] precosToInt = new Integer[precos.length];
				for (int i = 0; i < precos.length; i++) {
					precosToInt[i] = Integer.parseInt(precos[i]);
				}
				
				for (int i = 0; i < precosToInt.length; i++) {
					if(casoResolvido){
						break;
					}
					Integer precoI = precosToInt[i];
					for (int j = 0; j < precosToInt.length; j++) {
						Integer precoJ = precosToInt[j];
						
						if(precoI + precoJ == credito){
							System.out.println("Case #" + caso + ": " + (i+1) + " " + (j+1));
							caso++;
							casoResolvido = true;
							credito = 0;
							produtos = 0;
							precos = null;
							break;
						}
					}
				}
			}
			
			linha = br.readLine();
			contador++;
		}
		
		br.close();
	}

}
